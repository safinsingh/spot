{
	"name": "spot-grpc",
	"version": "0.1.0",
	"description": "eslint config for spot monorepo",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "ts-node-dev src/index.ts",
		"fmt": "prettier --write 'src/**/*.{js,ts,tsx,yml,scss,json}'",
		"fmt:ci": "prettier --check 'src/**/*.{js,ts,tsx,yml,scss,json}'",
		"lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
		"lint:ci": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
		"build": "rm -rf dist/* && tsc"
	},
	"keywords": [
		"eslint"
	],
	"author": "Safin Singh",
	"license": "AGPL-3.0",
	"dependencies": {
		"@grpc/grpc-js": "^1.2.2",
		"@grpc/proto-loader": "^0.5.5"
	},
	"devDependencies": {
		"@types/node": "14.14.16",
		"eslint": "7.16.0",
		"eslint-config-spot": "workspace:^0.1.0",
		"prettier": "2.2.1",
		"ts-node-dev": "1.1.1",
		"tslib": "2.0.3",
		"typescript": "4.1.3"
	}
}
